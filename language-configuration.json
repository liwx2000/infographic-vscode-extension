{
  "comments": {
    "lineComment": "#"
  },
  "brackets": [],
  "autoClosingPairs": [
    { "open": "\"", "close": "\"" },
    { "open": "'", "close": "'" }
  ],
  "surroundingPairs": [
    { "open": "\"", "close": "\"" },
    { "open": "'", "close": "'" }
  ],
  "indentationRules": {
    "increaseIndentPattern": "^\\s*(design|data|theme|items)\\s*$",
    "decreaseIndentPattern": "^\\s*$"
  },
  "wordPattern": "[a-zA-Z][a-zA-Z0-9_-]*"
}
